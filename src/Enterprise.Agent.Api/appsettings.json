{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/enterprise-agent-.txt",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "ModelProviders": {
    "Ollama": {
      "BaseUrl": "http://192.168.100.99:11434",
      "Timeout": "00:05:00"
    },
    "Qwen": {
      "BaseUrl": "https://dashscope.aliyuncs.com/api/v1/services/aigc/text-generation/generation",
      "ApiKey": "",
      "Timeout": "00:02:00"
    }
  },
  "DefaultModels": {
    "Chat": "ollama:qwen2.5:7b",
    "CodeAssistant": "ollama:deepseek-coder:6.7b",
    "DocumentAssistant": "qwen:qwen-turbo",
    "DataAnalyst": "qwen:qwen-plus"
  }
}